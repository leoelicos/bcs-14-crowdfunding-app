{{#each projects as |project|}}
  <div class='row align-center mb-5'>
    <div class='col-md-6'>
      <h2>{{{get_emoji}}} {{project.name}}</h2>
    </div>
    <div class='col-md-6'>
      <p>{{project.description}}</p>
      <p>{{project.date_created}}</p>
      <p>{{project.needed_funding}}</p>
      <p>{{project.user_id}}</p>
      <p>{{project.user.name}}</p>
    </div>
  </div>
{{/each}}